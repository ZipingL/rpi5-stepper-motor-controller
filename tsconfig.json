{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "./dist",
    "rootDir": "./src",
    "lib": [ "ES2015", "DOM", "DOM.Iterable" ],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "declaration": true,


  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
